{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
 "scripts": {
  "dev:watch": "tsc -w",
  "dev": "nodemon dist/server.js",
  "build": "tsc -p tsconfig.json",
  "start": "node dist/server.js",
  "lint": "eslint .",
  "prisma:generate": "prisma generate",
  "prisma:migrate": "prisma migrate deploy",
  "prisma:dev": "prisma migrate dev --name init",
